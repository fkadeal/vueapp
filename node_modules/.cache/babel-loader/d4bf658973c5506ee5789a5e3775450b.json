          </select>\r\n              </div>\r\n              <button>Twoot</button>\r\n                </form>\r\n      </div>\r\n      <div class=\"user-profile_twoots-wrapper\">\r\n          <TwootItem \r\n          v-for=\"twoot in user.twoots\" \r\n          :key=\"twoot.id\" \r\n          :username=\"user.username\" \r\n          :twoot=\"twoot\" \r\n          @favourite=\"toggleFavourite\" />\r\n      </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TwootItem from \"./TwootItem\";\r\nexport default {\r\n  name: \"Userprofile\",\r\n  components:{TwootItem},\r\n  data(){\r\n    return{\r\n      newTwootContent:'',\r\n      selectedTwootType:'instant',\r\n      twootTypes:[\r\n        {value: 'draft', name:'Draft'},\r\n        {value: 'instant', name:'Instant Twoot'}\r\n      ],\r\n      followers:0,\r\n      user:{\r\n        id:1,\r\n        username:'infkadeal',\r\n        firstName:'Fkadeal',\r\n        lastName:'Matiwos',\r\n        email:'infkadeal@etbaba.com',\r\n        isAdmin:true,\r\n        twoots:[\r\n            {id:1, content:'Twotter is Amazing'},\r\n            {id:2, content:\"Don't forget to subscribe to etbaba\"}\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  watch:{followers(newFollowerCount, oldFollowerCount){\r\n    if(oldFollowerCount < newFollowerCount){\r\n      console.log(`${this.user.username}  has gained ${newFollowerCount - oldFollowerCount} follower`)\r\n    }\r\n  }\r\n\r\n  },\r\n  computed:{\r\n    fulName(){\r\n      return `${this.user.firstName} ${this.user.lastName}`;\r\n    }\r\n  },\r\n  methods:{\r\n    followUser(){\r\n      this.followers++\r\n    },\r\n    toggleFavourite(id){\r\n        console.log(`Favourited Tweet #${id}`);\r\n    },\r\n        createNewTwoot(){\r\n            if(this.newTwootContent && this.selectedTwootType !=='draft'){\r\n                this.user.twoots.unshift({ \r\n                    id:this.user.twoots.length +1,\r\n                    content:this.newTwootContent\r\n                })\r\n                this.newTwootContent=''\r\n            }\r\n        }\r\n  },\r\n  mounted(){\r\n    this.followUser();\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.user-profile{\r\n    display: grid;\r\n    grid-template-columns: 1fr 3fr;\r\n    grid-gap:50px;\r\n    padding:50px 5%;\r\n}\r\n\r\n.user-profile_user-panel{\r\n    display:flex;\r\n    flex-direction:column;\r\n    padding:20px;\r\n    background-color:white;\r\n    border-radius: 5px;\r\n    border:1px solid #dfe3eb;\r\n}\r\n\r\n.user-profile_admin-badge{\r\n    margin-right:0;\r\n    background:rebeccapurple;\r\n    color:white;\r\n    border-radius:5px;\r\n    margin-bottom:20px;\r\n    padding:0 10px;\r\n    font-weight: bold;\r\n}\r\nh1{\r\n    margin:0;\r\n}\r\n.user-profile_twoots-wrapper{\r\n    display: grid;\r\n    grid-gap:10px;\r\n}\r\n.user-profile_creat-twoot{\r\n  border-top:1px solid #dfe3ef;\r\n  padding-top:20px;\r\n  display:flex;\r\n  flex-direction: column;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}