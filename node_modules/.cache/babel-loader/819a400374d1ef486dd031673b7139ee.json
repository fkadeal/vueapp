TA;AAlBO,CAAf","sourcesContent":["<template>\r\n    <form class=\"user-profile_creat-twoot\" @submit.prevent=\"createNewTwoot\" :class=\"{'--eceeded': newTwootCharacterCount > 180}\">\r\n            <label for=\"newTwoot\"><strong>New Twoot</strong>({{newTwootCharacterCount}}/180)</label>\r\n              <textarea id=\"newTwoot\" rows=\"4\" v-model=\"newTwootContent\"/>\r\n              \r\n              <div class=\"create-twoot-panel_submit\">\r\n              <div class=\"create-twoot-type\">\r\n                <label for=\"newTwootType\"><strong>Type: </strong></label>\r\n                <select id=\"newTwootType\" v-model=\"selectedTwootType\">\r\n                  <option :value=\"option.value\" \r\n                  v-for=\"(option, index) in twootTypes\" \r\n                  :key=\"index\">\r\n\r\n                  {{option.name}}\r\n                  </option>\r\n                  </select>\r\n              </div>\r\n              <button>Twoot It!</button>\r\n              </div>\r\n                </form>\r\n</template>\r\n<script>\r\nexport default {\r\n    name:\"CreateTwootPanel\",\r\n    data(){\r\n        return{\r\n            newTwootContent:'',\r\n            selectedTwootType:'instant',\r\n            twootTypes:[\r\n                {value: 'draft', name:'Draft'},\r\n                {value: 'instant', name:'Instant Twoot'}\r\n            ],\r\n        }\r\n    },\r\n   \r\n  computed:{\r\n    newTwootCharacterCount(){\r\n      return this.newTwootContent.length;\r\n    }\r\n  },\r\nmethods:{\r\n        createNewTwoot(){\r\n            if(this.newTwootContent && this.selectedTwootType !=='draft'){\r\n                this.user.twoots.unshift({ \r\n                    id:this.user.twoots.length +1,\r\n                    content:this.newTwootContent\r\n                })\r\n                this.newTwootContent=''\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.creat-twoot-panel{\r\n      margin-top:20px;\r\n      padding:20px 0;\r\n      display:flex;\r\n      flex-direction: column;\r\n      \r\n    textarea{\r\n        border:1px solid #dfe3e8;\r\n        border-radius:5px;\r\n    }\r\n\r\n    .create-twoot-panel_submit{\r\n        display: flex;\r\n        justify-content: space-between;\r\n\r\n        .create-twoot-type{\r\n            padding:10px 0;\r\n        }\r\n        \r\n        button{\r\n          margin: auto 0;\r\n          padding:5px 20px;\r\n          background-color:#07d807;\r\n          border-radius: 5px;\r\n          border:none;\r\n          color: white;\r\n          font-weight: bold;\r\n        }\r\n    }\r\n\r\n    &.--eceeded{\r\n        color: red;\r\n        border-color:red;\r\n        .create-twoot-panel_submit{\r\n            button{\r\n                background-color: red;\r\n                color:white;\r\n            }\r\n        }\r\n      }\r\n    }\r\n\r\n</style>"],"sourceRoot":""}]}