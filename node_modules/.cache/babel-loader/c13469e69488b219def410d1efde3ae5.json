KAhBN,EAAA,UAAA,EAgBM,CAfF,YAAA,CAAyD,IAAzD,EAAA,UAAA,EAAkC,MAAC,gBAAA,CAAE,KAAA,CAAA,IAAA,CAAK,QAAP,CAAnC,EAAkD;AAAA;AAAlD,GAeE,EAb8C,KAAA,CAAA,IAAA,CAAK,O,kBAAjD,YAAA,CAEK,KAFL,EAAA,UAAA,EAA0D,SAA1D,C,qCAaF,EAVC,YAAA,CAEG,KAFH,EAAA,UAAA,EAEG,CADF,UACE,E,kCAD2B,KAAA,CAAA,S,GAAS;AAAA;GACpC,CAFH,CAUD,EAPF,UAOE,CAhBN,CAyBE,EARF,YAAA,CAOM,KAPN,EAAA,UAAA,EAOM,E,kBANF,YAAA,CAK+B,SAL/B,EAK+B,IAL/B,EAK+B,WAAA,CAJf,KAAA,CAAA,IAAA,CAAK,MAIU,EAJJ,UAApB,KAAoB,EAAf;yBADZ,YAAA,CAK+B,oBAL/B,EAK+B;AAH9B,MAAA,GAAG,EAAE,KAAK,CAAC,EAGmB;AAF9B,MAAA,QAAQ,EAAE,KAAA,CAAA,IAAA,CAAK,QAEe;AAD9B,MAAA,KAAK,EAAE,KACuB;AAA9B,MAAA,WAAS,EAAE,QAAA,CAAA;AAAmB,KAL/B,E,IAAA,E;;AAAA,M,oCAAA,C;GAK+B,CAL/B,E;;AAAA,GAME,EAPN,CAQE,CA1BN,C","sourcesContent":["<template>\r\n  <div class=\"user-profile\">\r\n      <div class=\"user-profile_user-panel\">\r\n          <h1 class=\"user-profile_username\">@{{user.username}}</h1>\r\n          \r\n              <div class=\"user-profile_admin-badge\" v-if=\"user.isAdmin\">\r\n                  Admin\r\n             </div>\r\n             <div class=\"user-profile_follower-count\">\r\n              <strong>Followers:</strong>{{followers}}\r\n          </div>\r\n          <form class=\"user-profile_creat-twoot\">\r\n            <label for=\"newTwoot\"><strong>New Twoot</strong></label>\r\n              <textarea id=\"newTwoot\" rows=\"4\" />\r\n              <div class=\"user-profile_create-twoot-type\">\r\n                <label for=\"newTwootType\"><strong>Type: </strong></label>\r\n              </div>\r\n                </form>\r\n      </div>\r\n      <div class=\"user-profile_twoots-wrapper\">\r\n          <TwootItem \r\n          v-for=\"twoot in user.twoots\" \r\n          :key=\"twoot.id\" \r\n          :username=\"user.username\" \r\n          :twoot=\"twoot\" \r\n          @favourite=\"toggleFavourite\" />\r\n      </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TwootItem from \"./TwootItem\";\r\nexport default {\r\n  name: \"Userprofile\",\r\n  components:{TwootItem},\r\n  data(){\r\n    return{\r\n      followers:0,\r\n      user:{\r\n        id:1,\r\n        username:'infkadeal',\r\n        firstName:'Fkadeal',\r\n        lastName:'Matiwos',\r\n        email:'infkadeal@etbaba.com',\r\n        isAdmin:true,\r\n        twoots:[\r\n            {id:1, content:'Twotter is Amazing'},\r\n            {id:2, content:\"Don't forget to subscribe to etbaba\"}\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  watch:{followers(newFollowerCount, oldFollowerCount){\r\n    if(oldFollowerCount < newFollowerCount){\r\n      console.log(`${this.user.username}  has gained ${newFollowerCount - oldFollowerCount} follower`)\r\n    }\r\n  }\r\n\r\n  },\r\n  computed:{\r\n    fulName(){\r\n      return `${this.user.firstName} ${this.user.lastName}`;\r\n    }\r\n  },\r\n  methods:{\r\n    followUser(){\r\n      this.followers++\r\n    },\r\n    toggleFavourite(id){\r\n        console.log(`Favourited Tweet #${id}`);\r\n    }\r\n  },\r\n  mounted(){\r\n    this.followUser();\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n.user-profile{\r\n    display: grid;\r\n    grid-template-columns: 1fr 3fr;\r\n    grid-gap:50px;\r\n    padding:50px 5%;\r\n}\r\n\r\n.user-profile_user-panel{\r\n    display:flex;\r\n    flex-direction:column;\r\n    padding:20px;\r\n    background-color:white;\r\n    border-radius: 5px;\r\n    border:1px solid #dfe3eb;\r\n}\r\n\r\n.user-profile_admin-badge{\r\n    margin-right:0;\r\n    background:rebeccapurple;\r\n    color:white;\r\n    border-radius:5px;\r\n    margin-bottom:20px;\r\n    padding:0 10px;\r\n    font-weight: bold;\r\n}\r\nh1{\r\n    margin:0;\r\n}\r\n.user-profile_twoots-wrapper{\r\n    display: grid;\r\n    grid-gap:10px;\r\n}\r\n.user-profile_creat-twoot{\r\n  border-top:1px solid #dfe3ef;\r\n  padding-top:20px;\r\n  display:flex;\r\n  flex-direction: column;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}